<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/supercar_lambo_white">

    <View
        android:id="@+id/view2"
        android:layout_width="365dp"
        android:layout_height="50dp"
        android:layout_below="@+id/textView36"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/table_style" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="131dp"
        android:layout_height="22dp"
        android:layout_alignTop="@+id/view2"
        android:layout_alignBottom="@+id/view2"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/textView18"
        android:text="Income of the items"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="81dp"
        android:layout_height="21dp"
        android:layout_alignTop="@+id/view2"
        android:layout_alignBottom="@+id/view2"
        android:layout_marginStart="3dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/textView19"
        android:text="Name"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="72dp"
        android:layout_height="20dp"
        android:layout_alignStart="@+id/view2"
        android:layout_alignTop="@+id/view2"
        android:layout_alignBottom="@+id/view2"
        android:layout_marginStart="19dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="14dp"
        android:text="Code"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="169dp"
        android:text="@string/Sales"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView35"
        android:layout_alignStart="@+id/textView35"
        android:layout_marginStart="0dp"
        android:layout_marginTop="2dp"
        android:text="@string/SalesDS"
        android:textColor="@color/black"
        android:textSize="15sp" />

    <ListView
        android:id="@+id/SalesView"
        android:layout_width="367dp"
        android:layout_height="408dp"
        android:layout_below="@+id/view2"
        android:layout_alignStart="@+id/view2"
        android:layout_alignEnd="@+id/view2"
        android:layout_marginStart="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="0dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="#7B32F4"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

</RelativeLayout>